<launch>
  <!-- Projector on -->
  <include file="$(find pr2_camera_synchronizer)/projector_on.launch" />

  <node pkg="life_test" type="pr2_burn_in_switch.py" name="pr2_burn_in_switch"
        args="$(find life_test)/pr2_controllers.launch $(find base_life_test)/base_life_test.launch -d 0.5" 
  
  <!-- Monitor (with transmission checker) -->
  <node pkg="life_test" type="test_monitor.py"
        name="monitor" >
    <rosparam command="load"
              file="$(find life_test)/pr2_test/pr2_listeners.yaml" />
  </node>

  <node pkg="rostopic" type="rostopic" name="heartbeat_pub" 
        args="pub heartbeat std_msgs/Empty -r1" />
</launch>
