<!-- Stores data on every test that can run through Test Manager -->
<tests>
  <test serial="faketes" name="Simple Test" 
	short="Fake" trac="007"
	desc="Test to test life GUI"
	script="simple_test/test.launch" power="false"
	type="Life Test" >

    <!-- Load test parameters -->
    <param name="Rate" param_name="cycle_rate" 
	   desc="Changes the frequency of the test" 
	   rate="true" val="1.0" />

    <param name="Choice Param" param_name="test_choice"
	   desc="Chooses between a few choices" 
	   val="A" rate="false" />
  </test>

  <test serial="6804015" name="Gripper Test"
        short="Gripper" trac="000"
        desc="Test gripper moving in and out. Place dog toy within grasping range of gripper for first 1 hour. Test should run for 40 hours."
        script="gripper_test/test_gui.launch" power="true"
        type="Burn-in Test" >
    <param name="Rate" param_name="cycle_rate"
           desc="Frequency of test"
           rate="true" val="0.1" />
  </test>

    <test serial="6802478" name="Torso Test"
        short="Torso" trac="000"
        desc="Test torso moving up and down."
        script="torso_test/test_gui.launch" power="true"
        type="Burn-in Test" >
    <param name="Rate" param_name="cycle_rate"
           desc="Frequency of test"
           rate="true" val="0.03" />
  </test>

  <test serial="6804108" name="Caster Test"
        short="Caster" trac="001"
        desc="Caster jerks around under pressure. Put pressure on caster BEFORE turning this test on."
        script="caster_test/test_gui.launch" power="true"
        type="Burn-in Test" >
    <param name="Rate" param_name="cycle_rate"
           desc="Frequency of test"
           rate="true" val="2.0" />
  </test>

  
  <test serial="6804213" name="Free Wrist Test" 
	short="Wrist" trac="438"
	desc="Test of wrist and dummy gripper."
	script="wrist_test/test_gui.launch" 
	type="Burn-in Test" power="true" >
    <param name="Rate" param_name="cycle_rate"
	   desc="Frequency of commands"
	   val="3.0" rate="true" />
    <param name="Roll Effort" param_name="roll_effort"
	   desc="Wrist Roll Effort"
	   val="4.0" rate="false" />
  </test>

  <test serial="6804040" name="Head Life Test" 
	short="Head" trac="289"
	desc="Tests head on test cart, moving between [-0.4, 1.4] tilt, [-2.7, 2.7] pan."
	script="head_test/test_gui.launch"
	type="Life Test" power="true" >
    <param name="Cycle rate" param_name="cycle_rate"
	   desc="Cycle rate of head"
	   val="2.0" rate="true" />
  </test>

  <test serial="6804001" name="Laser Tilt Life Test" 
	short="Sled" trac="41"
	desc="Tests laser tilt with Hokuyo wire connection."
	script="laser_tilt_test/test_gui.launch"
	type="Life Test" power="true" >
  </test>
  
  <test serial="6804139" name="Upperarm Test"
	short="Upperarm" trac="400"
	desc="Tests upperarm."
	rate="1.0" script="upperarm_test/test_gui.launch"
	type="Burn-in Test" power="true" >
    <param name="Forearm Roll" param_name="forearm_roll"
	   desc="Forearm roll true/false"
	   val="true" rate="false" />
  </test>


  <!-- Not supported tests
  <test serial="6804178" name="Full Arm Counterbalance Test"
	short="Full CB" trac="402"
	desc="Moves arm up and down for full counterbalance workout."
	script="arm_life_test/test_gui_cb.launch"
	type="Life Test" power="true" >
    <param name="Cycle rate" param_name="cycle_rate"
	   desc="Cycle rate of arm/CB"
	   val="0.5" rate="true" />
    </test>

  <test serial="6804178" name="Full Arm CB Test - Dummy Forearm"
	short="Dummy CB" trac="402"
	desc="Moves arm up and down for full counterbalance workout."
	script="arm_life_test/test_gui_cb_forearm.launch"
	type="Life Test" power="true" >
    <param name="Cycle rate" param_name="cycle_rate"
	   desc="Cycle rate of arm/CB"
	   val="0.5" rate="true" />
  </test>
  -->
  
</tests>
