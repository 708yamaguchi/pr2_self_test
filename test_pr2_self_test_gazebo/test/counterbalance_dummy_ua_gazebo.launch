<launch>
  <!-- Gazebo and robot -->
  <include file="$(find test_pr2_self_test_gazebo)/gazebo_util/shoulder_dummy_ua_gazebo.launch" />

  <!-- Used for shoulder production test --> 
  <include file="$(find qualification)/tests/shoulder_test/counterbalance_dummy_ua.launch" />
  <!-- Expanding bounds for errors, for gazebo -->
  <group ns="test_controller" >
    <group ns="lift">
      <param name="mse" type="double" value="10.0" />
      <param name="avg_abs" type="double" value="5.0" />
      <param name="avg_eff" type="double" value="3.0" />
    </group>
  </group>
  
  <!-- Test: asserts we have result service and that we passed -->
   <test test-name="counterbalance_dummy_ua" time-limit="180" 
         pkg="test_pr2_self_test_gazebo" name="test_counterbalance_dummy_ua"
         type="check_pr2_self_test.py" />
   
</launch>
