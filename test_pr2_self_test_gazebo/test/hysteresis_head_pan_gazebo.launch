<launch>
  <!-- Gazebo and robot -->
  <include file="$(find test_pr2_self_test_gazebo)/gazebo_util/pr2_robot_gazebo.launch" />

  <include file="$(find qualification)/tests/head_test/hysteresis_head_pan.launch" />

  <group ns="test_controller" >
    <param name="max_position" type="double" value="2.70" />
    <param name="min_position" type="double" value="-2.70" />
  </group>
  
  <!-- Test: asserts we have checkout service and that we passed or human'd -->
  <test type="check_pr2_self_test.py" pkg="test_pr2_self_test_gazebo" name="test_hysteresis"
        args="--human_ok" test-name="pr2_head_pan_hysteresis" time-limit="300"  />
</launch>
