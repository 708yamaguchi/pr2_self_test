<launch>
  <machine name="realtime" user="root" address="$(env ROS_TEST_HOST)" ros-root="$(env ROS_ROOT)" ros-package-path="$(env ROS_PACKAGE_PATH)" default="never"/>   

  <machine name="localhost" address="localhost" ros-root="$(env ROS_ROOT)" ros-package-path="$(env ROS_PACKAGE_PATH)" default="true"/> 

  <!-- Record all diagnostics from qual tests -->
  <node pkg="rosrecord" type="rosrecord" args="-f /hwlog/qual_test_runtime_automatic /diagnostics" />

</launch>