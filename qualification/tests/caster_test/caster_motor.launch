<launch>
  <param name="robot_description" command="$(find xacro)/xacro.py '$(find qualification)/tests/caster_test/caster_motor.urdf.xacro'" />

  <!-- Loads realtime machine and PR2_etherCAT -->
  <include file="$(find qualification)/tests/init.machine" />
  <include file="$(find qualification)/tests/pr2_etherCAT.launch" />

  <group ns="cal_caster_motor" >
    <param name="type" type="str" value="MotorJointCalibrationController" />
    <param name="joint" type="str" value="caster_motor_joint" />
  </group>
  <node pkg="mechanism_bringup" type="calibrate.py"
        args="cal_caster_motor" name="cal_caster_motor" output="screen" />


</launch>
